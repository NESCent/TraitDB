<% @page_title = "CSV Import Config" %>
<div class="row-fluid">
  <div class="row-fluid"><p class="lead">CSV Import Config</p></div>
  <div class="row-fluid">
    <table class="table table-striped table-bordered">
      <tr>
        <th>File</th>
        <td><%= link_to(@config.config_file_file_name, @config.config_file.url) %></td>
      </tr>
      <tr>
        <th>Project</th>
        <td><% if @config.project %><%= @config.project.name %><% end %></td>
      </tr>
      <tr>
        <th>Owner</th>
        <td><%= @config.user.email %></td>
      </tr>
      <tr>
        <th>Created</th>
        <td><%= @config.created_at %></td>
      </tr>
      <tr>
        <th>Updated</th>
        <td><%= @config.updated_at %></td>
      </tr>
    </table>
  </div>
  <div class="row-fluid"><%= link_to("<< Back", csv_import_configs_path, :class => 'btn') %>
</div>
</div>
