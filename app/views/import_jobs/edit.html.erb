<% @page_title = "Edit Import Job" %>
<div class="row-fluid">
  <div class="row-fluid"><p class="lead">Update Import Job:</p></div>
  <div class="row-fluid">
    <%= form_for( @import_job, {:method => :put, :url => csv_dataset_import_job_path(@dataset.id, @import_job.id), }) do |f| %>
      <%= render(:partial => "form", :locals => { :f => f}) %>
      <div class="form-buttons">
        <%= submit_tag("Update import job", :class => 'btn btn-primary') %>
      </div>
    <% end %>
  </div>
  <div class="row-fluid"><%= link_to("<< Back", csv_dataset_import_jobs_path(@dataset), :class => 'btn') %></div>
</div>
