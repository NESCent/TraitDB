<% @page_title = "Edit CSV Import Config" %>
<div class="row-fluid">
  <div class="row-fluid"><p class="lead">Update config</p></div>
  <div class="row-fluid">
  <%= form_for(:config, :url => {:action => 'update', :id => @config.id }, :html => { :multipart => 'true', :class => "form" }) do |f| %>
    <%= render(:partial => "form", :locals => { :f => f }) %>
    <div class="form-actions">
    	<%= submit_tag("Update config", :class => 'btn btn-primary') %>
    </div>
  <% end %>
  </div>
  <div class="row-fluid"><%= link_to("<< Back", csv_import_configs_path, :class => 'btn') %></div>
</div>
