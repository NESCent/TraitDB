<% @page_title = "Edit Trait" %>
<div class="row-fluid">
  <div class="row-fluid"><p class="lead">Update Trait</p></div>
  <div class="row-fluid">
  <%= form_for(@continuous_trait, :url => {:action => 'update' }, :html => { :class => 'form' }) do |f| %>
    <%= render(:partial => 'form', :locals => { :f => f }) %>
    <div class="form-actions">
    	<%= submit_tag("Update Trait", :class => 'btn btn-primary') %>
    </div>
  <% end %>
  </div>
</div>
