<% @page_title = "Edit CSV Import Template" %>
<div class="row-fluid">
  <div class="row-fluid"><p class="lead">Update template</p></div>
  <div class="row-fluid">
  <%= form_for(:template, :url => {:action => 'update', :id => @template.id }, :html => { :multipart => 'true', :class => "form" }) do |f| %>
    <%= render(:partial => "form", :locals => { :f => f }) %>
    <div class="form-actions">
    	<%= submit_tag("Update template", :class => 'btn btn-primary') %>
    </div>
  <% end %>
  </div>
  <div class="row-fluid"><%= link_to("<< Back", csv_import_templates_path, :class => 'btn') %>
</div>
