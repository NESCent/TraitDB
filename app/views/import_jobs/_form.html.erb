<%= error_messages_for(@import_job) %>
<% fields_array = CsvDataset.fields.map { |field| [field, field] } %>
  <table>
    <tr>
      <th><%= f.label(:quantitative_header_start, "First Quantitative Header") %></th>
      <td><%= f.select(:quantitative_header_start, options_for_select(fields_array)) %></td>
    </tr>
    <tr>
      <th><%= f.label(:quantitative_header_end, "Last Quantitative Header") %></th>
      <td><%= f.select(:quantitative_header_end, options_for_select(fields_array)) %></td>
    </tr>
    <tr>
      <th><%= f.label(:qualitative_header_start, "First Qualitative Header") %></th>
      <td><%= f.select(:qualitative_header_start, options_for_select(fields_array)) %></td>
    </tr>
    <tr>
      <th><%= f.label(:qualitative_header_end, "Last Qualitative Header") %></th>
      <td><%= f.select(:qualitative_header_end, options_for_select(fields_array)) %></td>
    </tr>
  </table>
